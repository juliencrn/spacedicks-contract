{
  "name": "ntf-collectible",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "engines": {
    "node": "16.13.0"
  },
  "workspaces": [
    "./api",
    "./website"
  ],
  "scripts": {
    "clean": "lerna run clean",
    "hard-clean": "npm run clean && lerna clean -y",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build:api": "lerna run --scope api build",
    "heroku-postbuild": "npm run build:api",
    "start:api": "lerna run --scope api start",
    "dev": "concurrently --kill-others \"npm run dev:api\" \"npm run dev:site\"",
    "dev:api": "cd api && npm run dev --stream",
    "dev:site": "cd website && npm run dev --stream",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "NFT",
    "Typescript",
    "Rest",
    "Express",
    "dapp",
    "solidity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@truffle/hdwallet-provider": "^1.7.0",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "web3": "^1.6.0"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.15",
    "chai": "^4.3.4",
    "lerna": "^4.0.0",
    "truffle-assertions": "^0.9.2",
    "truffle-plugin-verify": "^0.5.18"
  }
}