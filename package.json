    "@openzeppelin/contracts": "^4.3.2",
    "@truffle/hdwallet-provider": "^1.7.0",
    "await-to-js": "^3.0.0",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "web3": "^1.6.0",
    "web3-utils": "^1.6.0"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.15",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "@types/web3": "^1.2.2",
    "chai": "^4.3.4",
    "nodemon": "^2.0.15",
