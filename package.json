{
  "name": "ntf-collectible",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "build:api": "cd api && tsc -p ./tsconfig.json",
    "build:site": "cd website && npm run build",
    "heroku-postbuild": "npm run build:api",
    "start:api": "node api/dist/api/src/server.js",
    "dev": "concurrently --kill-others \"npm run dev:api\" \"npm run dev:site\"",
    "dev:api": "nodemon api/src/server.ts",
    "dev:site": "cd website && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "NFT",
    "Typescript",
    "Rest",
    "Express",
    "dapp",
    "solidity"
  ],
  "author": "",
